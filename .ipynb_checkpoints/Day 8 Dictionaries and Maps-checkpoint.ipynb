    "import sys\n",
    "\n",
    "n = int(input().strip())\n",
    "phonebook = {}\n",
    "for i in range(0,n):\n",
    "    a,b = input().strip().split(' ')\n",
    "    phonebook.update({a:b})\n",
    "while True:\n",
    "    y = str(input().strip())\n",
    "    if y in phonebook.keys():\n",
    "        print(y+\"=\"+str(phonebook[y]))\n",
    "    else:\n",
    "        print(\"Not found\")\n"
   ]
