    "\n",
    "arr = list(map(int, input().strip().split(' ')))\n",
    "\n",
    "total = sum(arr)\n",
    "total_sum = []\n",
    "for item in arr:\n",
    "    total_sum.append(total - item)\n",
    "\n",
    "print(min(total_sum), max(total_sum))\n"
