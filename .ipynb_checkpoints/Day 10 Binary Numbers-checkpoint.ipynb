   "source": [
    "#!/bin/python3\n",
    "\n",
    "import sys\n",
    "\n",
    "n = int(input().strip())\n",
    "count = 0\n",
    "temp = 0\n",
    "for item in list(bin(n)[2:]):\n",
    "    if item == '1':\n",
    "        count += 1\n",
    "    else:\n",
    "        if count > temp: \n",
    "            temp = count\n",
    "        count = 0\n",
    "\n",
    "sys.stdout.write(str(max(temp,count)))"
   ]
  }
